{
    "name": "puli/assetic-extension",
    "description": "A Puli extension for the Assetic asset management library.",
    "homepage": "http://puli.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "puli/repository": "~1.0",
        "puli/twig-extension": "~1.0",
        "kriswallsmith/assetic": "~1.2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Puli\\Extension\\Assetic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puli\\Extension\\Assetic\\Tests\\": "tests/",
            "Puli\\Extension\\Twig\\Tests\\": "vendor/puli/twig-extension/tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "support": {
        "issues": "https://github.com/puli/issues/issues"
    }
}
